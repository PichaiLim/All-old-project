 <%


ConnectString = "Provider=Microsoft.Jet.OLEDB.4.0;Data Source="  & Server.MapPath("\") &"\jobs\db\jt_db2015.mdb"
Set conn = Server.CreateObject("ADODB.Connection")
conn.open ConnectString

Set rs = Server.CreateObject("ADODB.recordset")
rs.open "Select  * from jt_job",conn,1,3
 

%>   

<%
 if not rs.eof then
nrow=1


do while not rs.eof 
detailsx="details-"&nrow
registerx="register-"&nrow
collapsex="collapse-"&nrow

%>
<%if nrow mod 2 <>0 then%>            <div class="col-xs-12 col-md-6"> <%end if%>
                <div class="panel panel-primary">
                    <div class="panel-body">
                        <a class="panel-title-link" role="button" data-toggle="collapse" data-parent="#accordion" href="#<%=collapsex%>" aria-expanded="true" aria-controls="<%=collapsex%>" onClick="Collapse('<%=collapsex%>', <%=nrow%>);">
                            <h3 class="panel-title"><%=rs("PositionName")%> 
                           <% 
							select case rs("PositionState")
							case 1 response.Write(" <small class='label label-success' style='color:#fff;'>New</small>")
							case 2 response.Write(" <small class='label label-danger' style='color:#fff;'>Urgent</small>")
							end select
							%>
                            </h3>
                        </a>

                        <div class="clearfix">&nbsp;</div>

                        <div id="<%=collapsex%>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne" style="padding-left:1em;">
                            <p><strong>รายละเอียดงาน</strong></p>
                            <div style="padding-left:1em;">
                                <%=rs("PositionDescription")%>
                            </div>
                        </div>

                        <div class="clearfix">&nbsp;</div>

                        <div class="row">
                            <div class="col-xs-9" style="font-family:'sans-serif';">
                                <div class="text text-info">
                                    Update <%=rs("UpdateDate")%>
                                </div>
                            </div>
                            <div class="col-xs-3" style="border-left:5px solid rgba(195, 195, 195, 0.50);">
                                <div class="text text-right">
                                    <span id="<%=detailsx%>">
                                        <a class="panel-footer-link" role="button" data-toggle="collapse" data-parent="#accordion" href="#<%=collapsex%>" aria-expanded="true" aria-controls="<%=collapsex%>" onClick="Collapse('<%=collapsex%>', <%=nrow%>);">
                                            รายละเอียด <i class="fa fa-plus-square-o"></i>
                                        </a>
                                    </span>

                                    <span id="<%=registerx%>" class="hidden">
                                        <a class="panel-footer-link-register" href="register.asp?id=<%=rs("id")%>">
                                            สมัครงาน <i class="fa fa-check-square"></i>
                                        </a>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
               
                    
<%if nrow mod 2  = 0 then%>
</div>
<%end if%>
<%
nrow=nrow+1
 
rs.movenext
loop
